{
  "name": "javascript-module-boilerplate",
  "version": "2.1.0",
  "keywords": [
    "javascript",
    "node",
    "module",
    "boilerplate"
  ],
  "main": "dist/index.js",
  "browser": "dist-browser/index.js",
  "author": "Kennedy Rose <krose@escaladesports.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/escaladesports/javascript-module-boilerplate.git"
  },
  "license": "MIT",
  "scripts": {
    "analyze": "esca-scripts analyze --input './src/inject.js' --browser",
    "build": "run-p build:main build:browser",
    "build:main": "esca-scripts build --input './src/index.js' --output './dist/index.js' --component --minify false",
    "build:browser": "esca-scripts build --input './src/index.js' --output './dist-browser/index.js' --component --browser --name EscaEcomm",
    "dev": "esca-scripts dev ---input './dev/dev.js, ./dev/index.html' --browser",
    "open:github": "opn https://github.com/escaladesports/javascript-module-boilerplate",
    "prepublishOnly": "npm run build",
    "reset": "esca-scripts reset",
    "test": "esca-scripts test"
  },
  "devDependencies": {
    "esca-scripts": "^0.10.3",
    "npm-run-all": "^4.1.2"
  },
  "dependencies": {}
}
